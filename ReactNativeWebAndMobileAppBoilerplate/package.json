{
  "name": "ReactNativeWebAndMobileAppBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "run-ios": "react-native run-ios",
    "run-android": "react-native run-android",
    "run-web": "webpack-dev-server --config web/webpack.config.js --host localhost --disableHostCheck true",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "run:apk": "react-native run-android --variant=release",
    "runDebug:apk": "react-native run-android --variant=debug",
    "generate:apk": "cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "generateDebug:apk": "cd android && ./gradlew assembleDebug -x bundleReleaseJsAndAssets",
    "build:apk": "react-native bundle --platform android --entry-file index.js --reset-cache --bundle-output android/app/src/main/assets/index.android.bundle --dev false --assets-dest android/app/src/main/res/",
    "start:packager": "rm -f ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json && react-native start -- --reset-cache",
    "clear": "rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache clean && npm install",
    "start:dev": "node_modules/.bin/webpack-dev-server --inline --progress --port 8080",
    "start:test": "node_modules/.bin/webpack-dev-server --inline --progress --port 8080 --host 0.0.0.0 --disableHostCheck true"
  },
  "dependencies": {
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.0",
    "moment": "^2.20.1",
    "native-base": "^2.3.7",
    "react": "^16.4.0",
    "react-art": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-icons": "^2.2.7",
    "react-native": "0.55.4",
    "react-native-vector-icons": "^4.5.0",
    "react-native-web": "^0.8.3",
    "react-navigation": "^2.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.3.1",
    "react-toolbox": "^2.0.0-beta.12",
    "react-transition-group": "^1.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.0.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-native": "^4.0.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babylon": "^6.18.0",
    "css-loader": "^0.26.1",
    "escape-string-regexp": "^1.0.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "jest": "22.0.6",
    "metro-bundler": "^0.22.1",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^2.9.0",
    "postcss-each": "^0.9.3",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-mixins": "^5.4.1",
    "postcss-smart-import": "^0.7.6",
    "react-test-renderer": "16.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
